{
  "name": "Annotation Tool",
  "private": true,
  "dependencies": {
    "bootstrap-sass": "~3.3.5",
    "flat-ui": "~2.2.2",
    "flat-ui-sass": "~2.1.3",
    "modernizr": "~2.8.1",
    "select2": "~4.0.0"
  },
  "overrides": {
    "bootstrap-sass": {
      "main": [
        "assets/stylesheets/_bootstrap.scss",
        "assets/fonts/bootstrap/*",
        "assets/javascripts/bootstrap.js"
      ]
    },
    "flat-ui-sass": {
      "main":[
        "vendor/assets/stylesheets/flat-ui/_variables.scss",
        "vendor/assets/stylesheets/flat-ui.scss",
        "vendor/assets/fonts/flat-ui/*",
        "vendor/assets/javascripts/flat-ui/*"
      ]
    },
    "flat-ui": {
      "main": [
        "less/flat-ui.less",
        "dist/css/flat-ui.min.css",
        "dist/js/flat-ui.min.js"
      ]
    }
  },
  "devDependencies": {
    "chai": "~3.2.0",
    "mocha": "~2.2.5"
  }
}
