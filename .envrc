#!/usr/bin/env bash

while read line || [[ -n $line ]]
do
    workon $line
    echo work on $line
done < .venv

export PYTHONPATH=$PWD
echo export PYTHONPATH=$PWD

while read line || [[ -n $line ]]
do
    export $line
    echo export $line | awk -F "=" '{ gsub(/[a-zA-Z]/, "*", $2) }
                                   { gsub(/[0-9]/, "#", $2) }
                                   {print $1"="$2;}'
done < .env

